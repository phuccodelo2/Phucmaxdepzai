-- GrappleHook_RSpy_UI.lua -- Compact RSpy-style UI with a Grapple Hook tool and a Copy Code button. -- Place this as a LocalScript under StarterPlayerScripts or StarterGui (as a LocalScript in StarterGui). -- NOTE: setclipboard may only work in environments that expose it (Studio or some exploit clients).

local Players = game:GetService("Players") local UserInputService = game:GetService("UserInputService") local RunService = game:GetService("RunService") local LocalPlayer = Players.LocalPlayer local PlayerGui = LocalPlayer:WaitForChild("PlayerGui")

-- ====== CONFIG ====== local UI_NAME = "GrappleHook_RSpy_UI" local GRAPPLE_FORCE = 2000 -- higher = faster pull local STOP_DISTANCE = 6 -- ====================

-- The grapple core code as text (used for copy button). Keep concise and self-contained. local grappleCode = [[ -- Grapple Core local Players = game:GetService("Players") local RunService = game:GetService("RunService") local LocalPlayer = Players.LocalPlayer local mouse = LocalPlayer:GetMouse()

local function grappleTo(position) local char = LocalPlayer.Character if not char then return end local hrp = char:FindFirstChild("HumanoidRootPart") if not hrp then return end

local conn
local bodyVel = Instance.new("BodyVelocity")
bodyVel.MaxForce = Vector3.new(1e5,1e5,1e5)
bodyVel.Velocity = Vector3.new(0,0,0)
bodyVel.P = 5000
bodyVel.Parent = hrp

local function update()
    if not hrp or not hrp.Parent then
        if bodyVel then bodyVel:Destroy() end
        if conn then conn:Disconnect() end
        return
    end
    local dir = (position - hrp.Position)
    local dist = dir.Magnitude
    if dist <= STOP_DISTANCE then
        bodyVel:Destroy()
        if conn then conn:Disconnect() end
        return
    end
    bodyVel.Velocity = dir.Unit * GRAPPLE_FORCE
end

conn = RunService.Heartbeat:Connect(update)

end

-- Example usage: call grappleTo(mouse.Hit.Position) on click ]]

-- Inject distances into grappleCode for the copy text (so the pasted code is complete) grappleCode = string.format(grappleCode, STOP_DISTANCE, GRAPPLE_FORCE)

-- If UI already exists, remove it local existing = PlayerGui:FindFirstChild(UI_NAME) if existing then existing:Destroy() end

-- Helper to make instances quickly local function new(class, props) local obj = Instance.new(class) if props then for k,v in pairs(props) do obj[k] = v end end return obj end

-- ScreenGui local screenGui = new("ScreenGui", {Name = UI_NAME, ResetOnSpawn = false, Parent = PlayerGui})

-- Main frame (compact) local frame = new("Frame", { Parent = screenGui, Size = UDim2.new(0, 240, 0, 120), Position = UDim2.new(0.02, 0, 0.3, 0), BackgroundColor3 = Color3.fromRGB(30,30,30), BorderSizePixel = 0, AnchorPoint = Vector2.new(0,0), }) frame.Name = "MainFrame" frame.BackgroundTransparency = 0 frame.ClipsDescendants = true

-- Title local title = new("TextLabel", { Parent = frame, Size = UDim2.new(1, 0, 0, 28), Position = UDim2.new(0,0,0,0), BackgroundTransparency = 1, Text = "Grapple Hook (RSpy)", TextColor3 = Color3.new(1,1,1), Font = Enum.Font.SourceSansSemibold, TextSize = 16, TextXAlignment = Enum.TextXAlignment.Left, })

-- Close button (small) local closeBtn = new("TextButton", { Parent = frame, Size = UDim2.new(0, 24, 0, 24), Position = UDim2.new(1, -28, 0, 2), Text = "X", BackgroundColor3 = Color3.fromRGB(65,65,65), TextColor3 = Color3.new(1,1,1), Font = Enum.Font.SourceSansBold, TextSize = 14, }) closeBtn.MouseButton1Click:Connect(function() frame.Visible = false end)

-- Small status label local status = new("TextLabel", { Parent = frame, Size = UDim2.new(1, -12, 0, 22), Position = UDim2.new(0, 6, 0, 32), BackgroundTransparency = 1, Text = "Inactive", TextColor3 = Color3.fromRGB(200,200,200), Font = Enum.Font.SourceSans, TextSize = 14, TextXAlignment = Enum.TextXAlignment.Left, })

-- Toggle button local toggle = new("TextButton", { Parent = frame, Size = UDim2.new(0, 100, 0, 28), Position = UDim2.new(0, 6, 0, 60), Text = "Enable", BackgroundColor3 = Color3.fromRGB(85,170,85), TextColor3 = Color3.new(1,1,1), Font = Enum.Font.SourceSansBold, TextSize = 14, })

-- Copy button local copyBtn = new("TextButton", { Parent = frame, Size = UDim2.new(0, 100, 0, 28), Position = UDim2.new(0, 128, 0, 60), Text = "Copy Code", BackgroundColor3 = Color3.fromRGB(70,130,180), TextColor3 = Color3.new(1,1,1), Font = Enum.Font.SourceSansBold, TextSize = 14, })

-- Tiny helper notification (TextLabel that fades) local function notify(text) local n = new("TextLabel", { Parent = frame, Size = UDim2.new(1, -12, 0, 18), Position = UDim2.new(0,6,0,92), BackgroundTransparency = 1, Text = text, TextColor3 = Color3.fromRGB(200,200,200), Font = Enum.Font.SourceSansItalic, TextSize = 13, TextXAlignment = Enum.TextXAlignment.Left, }) delay(2, function() if n then n:Destroy() end end) end

-- Draggable frame local dragging, dragOffset frame.InputBegan:Connect(function(input) if input.UserInputType == Enum.UserInputType.MouseButton1 then dragging = true local mousePos = UserInputService:GetMouseLocation() dragOffset = Vector2.new(mousePos.X, mousePos.Y) - Vector2.new(frame.AbsolutePosition.X, frame.AbsolutePosition.Y) input.Changed:Connect(function() if input.UserInputState == Enum.UserInputState.End then dragging = false end end) end end) UserInputService.InputChanged:Connect(function(input) if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then local mousePos = UserInputService:GetMouseLocation() frame.Position = UDim2.new(0, mousePos.X - dragOffset.X, 0, mousePos.Y - dragOffset.Y) end end)

-- Grapple logic local enabled = false local mouse = LocalPlayer:GetMouse() local heartbeatConn

local function startGrappleMode() if enabled then return end enabled = true status.Text = "Active - Click to grapple" toggle.Text = "Disable" toggle.BackgroundColor3 = Color3.fromRGB(200,80,80)

heartbeatConn = mouse.Button1Down:Connect(function()
    local hit = mouse.Hit
    if not hit then return end
    local target = hit.p
    -- safety checks
    local char = LocalPlayer.Character
    if not char then return end
    local hrp = char:FindFirstChild("HumanoidRootPart")
    if not hrp then return end

    -- Create BodyVelocity
    local bv = Instance.new("BodyVelocity")
    bv.MaxForce = Vector3.new(1e5,1e5,1e5)
    bv.P = 5000
    bv.Velocity = Vector3.new(0,0,0)
    bv.Parent = hrp

    -- Pull until close
    local conn
    conn = RunService.Heartbeat:Connect(function()
        if not hrp or not hrp.Parent then
            if bv then bv:Destroy() end
            if conn then conn:Disconnect() end
            return
        end
        local dir = (target - hrp.Position)
        local dist = dir.Magnitude
        if dist <= STOP_DISTANCE then
            if bv then bv:Destroy() end
            if conn then conn:Disconnect() end
            return
        end
        bv.Velocity = dir.Unit * GRAPPLE_FORCE
    end)
end)

end

local function stopGrappleMode() if not enabled then return end enabled = false status.Text = "Inactive" toggle.Text = "Enable" toggle.BackgroundColor3 = Color3.fromRGB(85,170,85) if heartbeatConn then heartbeatConn:Disconnect() heartbeatConn = nil end end

toggle.MouseButton1Click:Connect(function() if enabled then stopGrappleMode() else startGrappleMode() end end)

-- Copy button behavior copyBtn.MouseButton1Click:Connect(function() local ok, err = pcall(function() if setclipboard then setclipboard(grappleCode) else error("no setclipboard") end end) if ok then notify("Code copied to clipboard.") else -- Fallback: show code in a small TextBox for manual copy local box = new("TextBox", { Parent = frame, Size = UDim2.new(1, -12, 0, 70), Position = UDim2.new(0,6,0,34), Text = grappleCode, TextWrapped = true, MultiLine = true, ClearTextOnFocus = false, TextEditable = true, TextXAlignment = Enum.TextXAlignment.Left, BackgroundColor3 = Color3.fromRGB(20,20,20), TextColor3 = Color3.fromRGB(230,230,230), }) -- Auto-select all text to make copying easier (studio/roblox may not allow selection programmatically) box:CaptureFocus() notify("setclipboard unavailable â€” select and copy the code shown.")

-- Remove box after 10s
    delay(12, function()
        if box then box:Destroy() end
    end)
end

end)

-- initial stopGrappleMode()

-- Optional: show a tiny toggle button to hide/show main UI (draggable) local toggleSmall = new("TextButton", { Parent = screenGui, Size = UDim2.new(0, 36, 0, 36), Position = UDim2.new(0, 6, 0, 140), Text = "GH", BackgroundColor3 = Color3.fromRGB(40,40,40), TextColor3 = Color3.new(1,1,1), Font = Enum.Font.SourceSansBold, TextSize = 14, }) local smallVisible = true toggleSmall.MouseButton1Click:Connect(function() smallVisible = not smallVisible frame.Visible = smallVisible end)

-- End of script

